generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Player {
  id          String   @id @default(cuid())
  rsn         String   @unique
  data        Json // this is lazy, and is a for-now approach.
  lastFetched DateTime

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
